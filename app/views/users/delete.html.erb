<% @page_title = "Delete Admin User" %>

<%= link_to  "<< Back to list", users_path, class:"back-link" %>

<div class="admin-users delete">
  <h2>Delete Admin Users</h2>
  <%= form_for @user do |f| %>
    <p>Are you sue you want to permanently delete this admin user?</p>

    <p class="reference-name"><%= f.object.name %></p>

    <div class="form-buttons">
      <%= f.submit 'Delete Admin User' %>
    </div>
  <% end %>

</div>